

@model IEnumerable<Model.Models.Warehouse.BookedWarehouseViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutWarehouse.cshtml";
}

<div class="row">
    <ol class="breadcrumb">
        <li>
            <a href="#">
                <em class="fa fa-home"></em>
            </a>
        </li>
        <li class="active">List</li>
    </ol>
</div>


<div class="row">
    <div class="col-md-12">

        @if (TempData["Success"] != null)
        {
            <p class="alert alert-success" id="successMessage">@TempData["Success"]</p>
            <script>
                var form = document.getElementById("AddDepartment");
                form.reset();
            </script>
        }

        @if (TempData["Failure"] != null)
        {
            <p class="alert alert-danger" id="failureMessage">@TempData["Failure"]</p>
        }

        <div class="panel panel-default articles">
            <div class="panel-body articles-container table-size">
                <div class="table-size-a rakeshcc" id="style-3">
                    <!--<h2 class="section-heading"></h2>-->
                    @*@Html.ActionLink("Add Department", "Add", "Department")*@

                    <table width="100%" id="table_id" class="table table-bordered table-hover">

                        <thead>
                            <tr>
                                @*<th>Department ID</th>*@
                                <th>Organization Name</th>
                                <th>Size Required</th>
                                <th>Category</th>

                                <th>Required From</th>
                                <th>Required To</th>
                                <th>Warehouse Description</th>
                                <th>Storage Type</th>
                                <th>Storage Area</th>
                                <th>Warehouse's'</th>
                                <th>Action</th>

                            </tr>
                        </thead>
                        <tbody>

                            @foreach (var lstEnterprise in Model)
                            {



                            <tr>
                                @if (!string.IsNullOrEmpty(lstEnterprise.WarehouseIds))
                                {
                                    <td>@lstEnterprise.OrganizationName </td>
                                    <td>@lstEnterprise.SizeRequired </td>
                                    <td>@lstEnterprise.Category</td>

                                    <td>@lstEnterprise.RequiredFrom.ToString("dd-MMM-yyyy") </td>
                                    <td>@lstEnterprise.RequiredTo.ToString("dd-MMM-yyyy") </td>
                                    <td>@lstEnterprise.WarehouseDescription </td>
                                    <td>@lstEnterprise.StorageType </td>
                                    <td>@lstEnterprise.StorageArea </td>
                                    <td>@lstEnterprise.WarehouseIds </td>
                                    <td>
                                        @Html.ActionLink("View", "RequestedWarehouse", new { BookingId = lstEnterprise.BookingId })
                                        @*@Html.ActionLink("Delete", "Delete", new { id = lstEnterprise.BookingId }, new { onclick = "return confirm('Are you sure you want to delete this record?');" })*@
                                    </td>
                                }

                            </tr>
                            }
                        </tbody>
                    </table>



                </div>

            </div>
        </div>

    </div>

    <!-- content ene -->

</div>
@*<script src="~/Scripts/customScripts.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>*@
<script type="text/javascript">
    $(document).ready(function () {
        $('#table_id').DataTable({
            responsive: true,
            ordering: true
        });
    });
</script>




